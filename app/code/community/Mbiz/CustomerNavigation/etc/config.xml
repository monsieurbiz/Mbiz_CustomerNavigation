<config>
    <modules>
        <Mbiz_CustomerNavigation>
            <version>0.1.0</version>
            <depends>
                <Mage_Customer />
            </depends>
        </Mbiz_CustomerNavigation>
    </modules>
    <global>
        <blocks>
            <customer>
                <rewrite>
                    <account_navigation>Mbiz_CustomerNavigation_Block_Account_Navigation</account_navigation>
                </rewrite>
            </customer>
            <mbiz_customernavigation>
                <class>Mbiz_CustomerNavigation_Block</class>
            </mbiz_customernavigation>
        </blocks>
    </global>
</config>